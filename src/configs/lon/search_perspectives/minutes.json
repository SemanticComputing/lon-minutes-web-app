        "sort_by_url": {
            "sortByPattern": "{ SELECT ?id (COALESCE(?_orderBy,0) AS ?orderBy) WHERE { VALUES ?facetClass { <FACET_CLASS> } ?id a ?facetClass . BIND (STR(?url) AS ?_orderBy) } }"
