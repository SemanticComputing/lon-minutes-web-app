                "PeopleInMinutes": {
                    "sparqlQuery": "peopleInMinutesQuery",
                    "targetClass": ":Minute",
                    "filterTarget": "minute__id",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.0,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        },
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        }
                    ]
                },
                "OrganizationsInMinutes": {
                    "sparqlQuery": "organizationsInMinutesQuery",
                    "targetClass": ":Minute",
