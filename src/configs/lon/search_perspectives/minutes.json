                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        },
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        }
                    ]
                },
                "ConceptsInMinutes": {
                    "sparqlQuery": "conceptsInMinutesQuery",
                    "targetClass": ":Minute",
                    "filterTarget": "minute__id",
                    "resultMapper": "mapPieChart",
                    "sliceVisibilityThreshold": 0.01,
                    "dropdownForChartTypes": "true",
                    "chartTypes": [
                        {
                            "id": "bar",
                            "createChartData": "createApexBarChartData"
                        },
                        {
                            "id": "pie",
                            "createChartData": "createApexPieChartData"
                        }
                    ]
                }
            }
        },
        "activitiesByPlace": {
            "tabID": 3,
            "component": "LeafletMap",
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "minutePlacesQuery",
            "facetClass": "minutes",
            "filterTarget": "minute",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInfoWindow",
                "relatedInstances": "minutesRelatedTo",
                "createPopUpContent": "createPopUpContentLoN"
            }
        },
        "placesMinutesHeatmap": {
            "tabID": 4,
            "tabPath": "heatmap",
