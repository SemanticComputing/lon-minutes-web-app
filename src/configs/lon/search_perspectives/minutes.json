            "facetClass": "minutes",
            "filterTarget": "minute",
            "resultMapper": "mapLineChart",
            "resultMapperConfig": {
                "fillEmptyValues": false
            },
            "createChartData": "createSingleLineChartData",
            "title": "Minutes by year",
