                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "born",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": true,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
                            },
                            {
                                "id": "deceased",
