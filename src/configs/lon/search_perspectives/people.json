    "URITemplate": "<BASE_URI>/actors/<LOCAL_ID>",
    "facetClass": "crm:E21_Person",
    "defaultConstraint": "FILTER EXISTS { [] :refers_to <SUBJECT> }",
    "frontPageImage": "main_page/people_lon2.jpg",
    "searchMode": "faceted-search",
