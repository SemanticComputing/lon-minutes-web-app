                        "tabIcon": "MailOutline",
                        "fetchResultsWhenMounted": true,
                        "sparqlQuery": "peopleMinutesInstancePageQuery",
                        "properties": [
                            {
                                "id": "minute",
                                "valueType": "object",
                                "makeLink": true,
                                "externalLink": false,
                                "sortValues": true,
                                "numberedList": true,
                                "onlyOnInstancePage": true,
                                "expandedOnInstancePage": true
